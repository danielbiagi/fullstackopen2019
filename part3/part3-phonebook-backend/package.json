{
  "name": "part3-phonebook-backend",
  "version": "1.0.0",
  "description": "Exercise for fullstackopen2019",
  "main": "index.js",
  "engines": {
    "node": "10.16.3"
  },
  "scripts": {
    "lint": "eslint .",
    "start": "node index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
