    "morgan": "^1.9.1",
    "morgan-body": "^2.4.7"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.3.0",
    "eslint": "^6.4.0",
    "nodemon": "^1.19.1"
  }
