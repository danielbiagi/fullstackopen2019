{
  "name": "part4-blog",
  "version": "1.0.0",
  "description": "Exercise for FullstackOpen2019",
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "start": "NODE_ENV=production node index.js",
    "watch": "NODE_ENV=development nodemon index.js",
    "test": "NODE_ENV=test jest --verbose --runInBand --detectOpenHandles",
